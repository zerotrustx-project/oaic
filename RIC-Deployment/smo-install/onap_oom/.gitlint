[title-match-regex]
regex=^\[[A-Z\d-]+]\[*[\w\d-]*]* [A-Z]+[\S ]*$

[body-match-regex]
regex=.{20,}

[ignore-by-title]
regex=^Release(.*)
ignore=title-match-regex,body-min-length

[ignore-body-lines]
regex=^(Issue-ID|Signed-off-by|Change-Id|Reported-by|Depends-on|Co-authored-by|Fixes|Acked-by|Reviewed-by|CC)

